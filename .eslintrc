{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "strict": 0,
        "quotes": [2, "single", "avoid-escape"],
        "no-else-return": 2,
        "no-underscore-dangle": 2
    }
}
